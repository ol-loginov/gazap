<!DOCTYPE html>
<html>
    $eCdn.setAppZone('/worlds')
<head>
    #parse('shared/html-head.vm')
    <title>#springMessage('LandingPage.title')</title>
</head>
#* @vtlvariable name="worldList" type="java.util.List<waypalm.domain.entity.World>" *#
<body id="bodyColumns" class="layout-1-4">
<div class="content">
    <div class="left">
        #set($landingSidebarActiveTab='landingSidebarCommon')
        #parse('_landingSidebar.vm')
    </div>
    <div class="center">
        <div class="content">
            <ul>
                #foreach($world in $worldList)
                    <li><a href="$cp/w/$world.slug">$escTool.html($world.title)</a></li>
                #end
            </ul>
        </div>
    </div>
</div>
</body>
</html>